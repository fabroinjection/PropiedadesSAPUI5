<core:FragmentDefinition xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:form="sap.ui.layout.form"
    >
    <Dialog id="helloDialog">
        <content>
            <form:SimpleForm id="SimpleFormDisplay480" editable="false" layout="ResponsiveGridLayout" title="Propiedad" labelSpanL="4" labelSpanM="4" emptySpanL="0" emptySpanM="0" columnsL="2" columnsM="2">
                <form:content>
                    <Label id="lbl1" text="Nombre Dueño" />
                    <Input id="input1" value="{PropiedadCreate>/NombreDuenio}" valueLiveUpdate="true" />
                    <Label id="lbl2" text="Apellido Dueño" />
                    <Input id="input2" value="{PropiedadCreate>/ApellidoDuenio}" valueLiveUpdate="true"/>
                    <Label id="lbl3" text="Tipo Propiedad" />
                    <Input id="input3" value="{PropiedadCreate>/TipoProp}" valueLiveUpdate="true"/>
                    <Label id="_IDGenLabel1" text="Superficie" />
                    <Input id="inputNumber" type="Number" value="{PropiedadCreate>/Superficie}" class="sapUiSmallMarginBottom" />
                    <Label id="lbl4" text="Nombre Calle" />
                    <Input id="input4" value="{PropiedadCreate>/NombreCalle}" valueLiveUpdate="true"/>
                    <Label id="lbl5" text="Número Calle" />
                    <Input id="input5" value="{PropiedadCreate>/NumeroCalle}" valueLiveUpdate="true"/>
                    <Label id="lbl6" text="Provincia" />
                    <Input id="input6" value="{PropiedadCreate>/Provincia}" valueLiveUpdate="true"/>
                    <Label id="lbl7" text="Localidad" />
                    <Input id="input7" value="{PropiedadCreate>/Localidad}" valueLiveUpdate="true"/>
                    <Label id="_IDGenLabel2" text="Número Habitaciones" />
                    <Input id="inputNumber2" type="Number" value="{PropiedadCreate>/NumHabitaciones}" class="sapUiSmallMarginBottom" />
                    <Label id="_IDGenLabel3" text="Precio" />
                    <Input id="inputNumber3" type="Number" value="{PropiedadCreate>/Precio}" class="sapUiSmallMarginBottom" />
                </form:content>
                <form:layoutData>
                    <FlexItemData id="_IDGenFlexItemData1" shrinkFactor="0" backgroundDesign="Solid" styleClass="sapContrastPlus"/>
                </form:layoutData>
            </form:SimpleForm>
        </content>
        <beginButton>
            <Button id="_IDGenButton2" type="Reject" text="Cancelar" press="onCerrarCrearPropiedadPress" />
        </beginButton>
        <endButton>
            <Button id="_IDGenButton1" type="Accept" text="Crear" press="onCrearPropiedadPress" />
        </endButton>
    </Dialog>
</core:FragmentDefinition>